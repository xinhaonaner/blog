{
    "name": "cnvs/easel",
    "description": "Canvas Framework",
    "keywords": [
        "canvas",
        "framework"
    ],
    "type": "canvas-framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Todd Austin",
            "email": "austin.todd.j@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "^2.5",
        "laravel/scout": "5.*",
        "erusev/parsedown": "^1.6",
        "maatwebsite/excel": "^2.1",
        "illuminate/support": "~5.4",
        "larapack/config-writer": "^1.0",
        "talvbansal/media-manager": "^2.5",
        "barryvdh/laravel-ide-helper": "^2.3",
        "teamtnt/laravel-scout-tntsearch-driver": "3.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "~1.4",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Canvas\\": "src/"
        }
    },
    "suggest": {
        "cnvs/paper-dark": "Official dark theme for Canvas (~1.0)."
    },
    "config": {
        "sort-packages": true
    }
}
